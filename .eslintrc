{

  /*
  * Based on AirBnb JavaScript style guide
  * Source : https://raw.githubusercontent.com/purposeindustries/styleguide/master/.eslintrc
  * Eslint rules: http://eslint.org/docs/rules
  */

  "env": {
    "browser": true,
    "jquery": true
  },

  "parserOptions": {
    "ecmaVersion": 6
  },

  "globals": {
    "define": true,
    "require": true,
    "module": true
  },

  "rules": {
    "array-bracket-spacing": [2, "never"],
    "block-scoped-var": 1,
    "brace-style": [2, "1tbs"],
    "camelcase": 1,
    "comma-dangle": [2, "never"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "complexity": [1,5],
    "consistent-return": 0,
    "consistent-this": [2, "_this"],
    "curly": [2, "all"],
    "dot-location": [2, "property"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": 0,
    "indent": [2, 2, { "VariableDeclarator": { "var": 2, "let": 2, "const": 3 }, "SwitchCase": 1 }],
    "key-spacing": [2, {afterColon: true, beforeColon: false}],
    "keyword-spacing": 2,
    "max-len": [1, 80, 4],
    "max-nested-callbacks": [2, 4],
    "new-cap": [2, { "newIsCap": true, "capIsNew": false }],
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "functions"],
    "no-extra-semi": 2,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-func-assign": 2,
    "no-global-strict": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 1,
    "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 1}],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-path-concat": 2,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-shadow-restricted-names": 2,
    "no-space-after-function": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, { "vars": "local", "args": "after-used" }],
    "no-warning-comments": 0,
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "one-var": 2,
    "prefer-const": ["error"],
    "quote-props": [1, "as-needed", { "keywords": true, "unnecessary": false }],
    "quotes": [2, "single", "avoid-escape"],
    "radix": 2,
    "semi-spacing": [2, { "before": false, "after": true }],
    "semi": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "outside"],
    "wrap-regex": 2
  }
}
